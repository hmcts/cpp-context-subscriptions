<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="drop-process-event-index1" author="Neha">
        <preConditions onFail="CONTINUE">
            <indexExists indexName="idx_processed_event_source"/>
        </preConditions>
        <dropIndex tableName="processed_event" indexName="idx_processed_event_source"/>
    </changeSet>
    <changeSet id="drop-process-event-index2" author="Neha">
        <preConditions onFail="CONTINUE">
            <indexExists indexName="idx_processed_event_event_number"/>
        </preConditions>
        <dropIndex tableName="processed_event" indexName="idx_processed_event_event_number"/>
    </changeSet>
</databaseChangeLog>